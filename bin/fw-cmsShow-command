#!/bin/sh

# export DISPLAY=:0.0

dataFile=$1;
ln -s -f ${FW_DIR}/log/cmsShow.$$.log ${FW_DIR}/log/cmsShow.log

options=" --log 0 --chain 10  --port $FW_PORT -p 2 --live"

echo "Starting cmsShow $options $dataFile";

/home/vis/cmsShow-7.0/cmsShow $options $dataFile >  ${FW_DIR}/log/cmsShow.$$.log 2>&1
